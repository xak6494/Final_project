@model IEnumerable<DataLayer.tstTechNote>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Notation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NotationDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tstEmployee.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tstTicket.TroubleDescription)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Notation)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NotationDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tstEmployee.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tstTicket.TroubleDescription)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
